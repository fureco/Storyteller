{
  "name": "storyteller",
  "productName": "Storyteller",
  "version": "1.0.0",
  "description": "A writing application build with Electron, React, PostCSS and Webpack.",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/alexdevero/electron-react-webpack-boilerplate.git"
